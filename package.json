{
  "name": "overlaymenu",
  "version": "1.2.8",
  "author": "Abdhesh Nayak",
  "license": "MIT",
  "description": "A easy to use react popup or modal handling component.",
  "keywords": [
    "react",
    "modal",
    "popup",
    "overlay-screen",
    "menu",
    "modal",
    "ui"
  ],
  "private": false,
  "types": "./dist/@types/index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    "./dist/index.css": {
      "import": "./dist/index.css",
      "require": "./dist/index.css"
    },
    ".": {
      "import": {
        "default": "./dist/index.mjs"
      },
      "require": {
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abdheshnayak/react-overlaymenu.git"
  },
  "scripts": {
    "build": "npm run clean && node build.js && npm run tsc",
    "pack": "npm pack --pack-destination ./out",
    "clean": "rimraf dist",
    "dev:build": "npm run build",
    "tsc": "tsc --emitDeclarationOnly --outDir ./dist/@types"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/react": "^18.2.22",
    "@types/react-dom": "^18.2.7",
    "@types/uuid": "^9.0.4",
    "@types/websocket": "^1.0.6",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "esbuild": "^0.19.2",
    "esbuild-sass-plugin": "^2.15.0",
    "eslint": "^8.47.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-jsconfig": "^1.1.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prefer-arrow-functions": "^3.1.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.1",
    "typescript": "^5.2.2"
  }
}
